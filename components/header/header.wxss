/* components/header/header.wxss */

.header{
  width: 100%;
  display: flex;
  position: fixed;
  z-index: 100;
  top: 0;
  left: 0;
}
.lefticon{
  height: 28px;
  background: rgba(0,0,0,0.2);
  width: 150rpx;
  position: absolute;
  left: 26rpx;
  border-radius: 14px;
  color: rgba(0,0,0,0.5);
  line-height: 28px;
  display: flex;
  justify-content: center;
}
.iconfont{
  color: #fff;
  font-size: 18px;
  flex-grow: 1;
  text-align: center;
}
.background{
  /* background: rgba(255,255,0,0.3); */
  width: 100%;
  /* opacity: 0; */
}
.background view{
  display: flex;
  align-items: center;
  justify-content: center;
}


